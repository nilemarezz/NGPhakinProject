<% include partial/header%>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<div class="container" style="margin-top:10%">
    <div class="row">
        <div class="col-md-6 col-sm-6 margin">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <img src="<%=list.pic%>" style="width:450px;height:300px;" />
            </div>
        </div>
        <br /><br />
        <div class="col-md-6 col-sm-6">
            <div class="product_title">
                <p><%= list.name%></p>
            </div>
            <div class="product_price">
                <p>Price: 2,500</p>
            </div>
            <div class="border_line"></div>
            <div class="product_desc">
                <p class="head">Description</p>
                <p class="lorem"><%= list.description%></p>
            </div>
            <div class="border_line"></div>
            <div class="product_desc">
                <p class="head">Score : </p>
                
            </div>
            <div class="border_line"></div>
            <br>
            <div class="product_desc">
                <p class="head">Link</p>
                <p class="lorem">
                    <div class="social-btn">
                    
                    <a href="#" class="fa fa-facebook"></a>
                    <a href="#" class="fa fa-instagram"></a>
                    <a href="#" class="fa fa fa-google"></a>
                    <a href="#" class="fa fa-twitter"></a>
                    

                </div>
            </div>
            
            
        </div>
    </div>

    <% include partial/loadeffect%>
</div>