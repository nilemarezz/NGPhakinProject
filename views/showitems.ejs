<% include partial/header%>
<div class="headtell">
    <h1 class="display-4 p-4"><i class="fas fa-camera-retro"></i>NG Phakin</h1>
    <p class="lead">
        Love is Eatting!! We collect the picture of all Food and Dessert we have
        eat.
    </p>
    <div class="addbutton">
        <a class="nav-link" href="/addform">Add Item</a>
    </div>
</div>

<div id="wrapper">
    <div class="container" style="margin-top:15px;">
        <div class="row">
            <% for(var i =0;i < list.length ; i++){ %>
            <div class="col-lg-3 col-md-6 " style="padding-top:10px;">
                <img src="<%= list[i].pic %>" class="img-thumbnail" style="width:350px;height:180px;" />

                <div id="showbox">
                    <h5><strong><%= list[i].name %></strong></h5>
                    <button type="button" class="btn btn-primary btn-sm">Detail</button>
                </div>
            </div>



            <%}%>

    </div>
  </div>
  


  
</div>
